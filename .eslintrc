{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },

  "rules": {
    "strict": "global",
    "quotes": [1, "single"],
    "no-trailing-spaces": 1,
    "eol-last": 1,
    "no-use-before-define": "nofunc",
    "no-unused-vars": 2
  }
}
